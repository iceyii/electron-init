{
  "name": "electron-init",
  "version": "1.0.0",
  "main": "electron/app.js",
  "scripts": {
    "dev": "concurrently -n \"⚡️ Vite,⚛ Electron\" -c \"yellow.bold,blue.bold\" \"cross-env BROWSER=none npm run electron:vite\" \"npm run electron\"",
    "build": "npm run build:electron && npm run electron:dist",
    "electron": "wait-on tcp:3000 && cross-env IS_DEV=true electron .",
    "electron:vite": "vite",
    "electron:dist": "electron-builder",
    "build:electron": "cross-env ELECTRON=true vite build"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "dexie": "^3.2.2",
    "dexie-mongoify": "^1.3.0",
    "element-plus": "^2.2.0",
    "monaco-editor": "^0.33.0",
    "vue": "^3.2.33",
    "vue-cropperjs": "^5.0.0",
    "vue-router": "^4.0.10",
    "vuex": "^4.0.2",
    "wangeditor": "^4.7.4"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.1.2",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "electron": "^18.0.4",
    "electron-builder": "^23.0.2",
    "vite": "2.9.9",
    "wait-on": "^6.0.1",
    "vite-plugin-monaco-editor": "^1.0.10"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
